import java.util.Scanner;

public class Multiplos {

	@SuppressWarnings("resource")
	public static void main(String[] args) {
		//PROGRAMA QUE LEA UN NUMERO POR TECLADO Y MUESTRE POR PANTALLA LOS MULTIPLOS DE 6
		//DESDE EL 1 HASTA EL NUMERO METIDO POR TECLADO


		/*Ejemplo
		 * 		Introduce un numero: 23
		 * 		Los multiplos del 6 hasta el 23 son:
		 * 		6,12, 18
		 */

		Scanner se;
		se=new Scanner (System.in);

		//VARIABLES
		int num, num1=0, cont=0, b;


		//NUMERO PEDIDO
		System.out.println("Introduce el multiplo que quieras:");
		b=se.nextInt();
		System.out.println("Introce un numero:");
		num=se.nextInt();
		do {
			num1=num1+b;
			System.out.print(num1+",");
		} while (num1<num);
		
		

	}






}


